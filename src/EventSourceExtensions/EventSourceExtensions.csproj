<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <PackageId>EventSourceExtensions</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>aelij</Authors>
    <Description>A library for generating EventSource classes from interfaces at run-time</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <ProjectUrl>https://github.com/aelij/EventSourceExtensions</ProjectUrl>
    <LicenseUrl>https://raw.githubusercontent.com/aelij/EventSourceExtensions/master/LICENSE</LicenseUrl>
    <PackageReleaseNotes />
    <Copyright>Copyright 2016</Copyright>
    <PackageTags>eventsource etw generator</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="NETStandard.Library" Version="1.6" />
    <PackageReference Include="System.Collections.Immutable" Version="1.3.1" />
    <PackageReference Include="System.Linq.Expressions" Version="4.3.0" />
    <PackageReference Include="System.Reflection.Emit" Version="4.3.0" />
  </ItemGroup>
</Project>